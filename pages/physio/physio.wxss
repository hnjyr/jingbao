/* pages/physio/physio.wxss */
page {
  background-color: #f1f1f1;
  padding: 20rpx;
}
.ha_info {
  border-radius: 12rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
  border: 1rpx solid #ddd;
}
.select_time {
  background-color: #fff;
  width: 100vw;
  margin-left: -20rpx;
  padding: 30rpx;
  margin-bottom: 40rpx;
}
.time_h2 {
  font-size: 34rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #333333;
  margin-bottom: 20rpx;
}
.scroll {
  width: 100%;
  display: flex;
  align-items: baseline;
  margin-bottom: 20rpx;
}
.ti_li {
  flex-shrink: 0;
  padding: 20rpx;
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #3E3E3E;
  border: 1rpx solid #DCDCDC;
  margin-right: 24rpx;
  border-radius: 8rpx;
}
.ti_li text {
  display: block;
  text-align: center;
  margin-bottom: 10rpx;
}
.ti_li text:first-child {
  font-size: 30rpx;
  font-weight: 500;
}
.ti_li.active {
  color: #2963E0;
  border: 1rpx solid #2963E0;
}

.ti_btn {
  width: 600rpx;
  position: fixed;
  bottom: 50rpx;
  left: 75rpx;
  background-color: #2963E0;
  color: #fff;
  font-size: 32rpx;
  border-radius: 50rpx;
  text-align: center;
  padding: 20rpx 0;
}

.time_h2 {
  font-size: 32rpx;
  color: #333;
  font-weight: 500;
}
.sele_time {
  width: 100%;
  display: flex;
  align-items: baseline;
}
.time_li {
  border: 1rpx solid #333;
  border-radius: 8rpx;
  padding: 12rpx 20rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
  font-size: 26rpx;
}
.activeTime {
  border-color: #2963E0;
  color: #2963E0;
}
.project {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pro_box {
  display: flex;
  justify-content: space-between;
}
.pro_box image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  /* background-color: #ccc; */
}
.pro_info {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.pro_info .pro_h2 {
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
}
.pro_info .pro_num {
  font-size: 24rpx;
  color: #ccc;
} 
.pro_info .pro_num text {
  color: #FF8400;
}