<!--pages/admin/bill.wxml-->
<view class="container">
  <view class="it" wx:for='{{list}}' wx:key='*this'>
    <view class="it" style="border:none;padding:0;">
      <image src="/images/banner.png" style="width:40rpx;height:60rpx;margin-right:30rpx;"></image>
       <view>
          <view style="font-size:30rpx;color:#999;margin-top:-20rpx;">{{item.body}}</view>
       </view>
    </view>
    <text>{{item.isRecharge==1?'+':'-'}}{{item.amount}}</text>
  </view>
</view>
