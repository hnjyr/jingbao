		</view>
	</view>

	<view class="userinfo">
		<view class="u_oneline flex_between">
			<view class="u_oneline_left flex">
				<image src="/images/admin/ava.png" style="width:117.3rpx;height:117.3rpx;margin-right:18.63rpx;"></image>
