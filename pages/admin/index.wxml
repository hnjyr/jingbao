      </view>
      <van-icon name="arrow" size="20px" color="#CCCCCC" />
    </view>
    <view class="b_it flex_between">
      <view class="b_it_left flex">
        <image src="/images/admin/guanyu.png" style="width:36rpx;height:36rpx;margin-right:37rpx;"></image>
