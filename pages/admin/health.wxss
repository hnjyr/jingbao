/* pages/admin/health.wxss */
.container{
  margin: 0 40rpx;
}
.input_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
  border-bottom: 1rpx solid #eeeeee;
  font-size: 28rpx;
  color: #555555;
}
.input_box text{
  width:140rpx;

}
.input_box input{
    flex: 1;
}
.input_box2{
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #555555;
}
.input_box2 .image_box{
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.input_box2 .image_box .imgSize{
    width: 322rpx;
    height: 200rpx;
}
.btn{
  color:#fff;
  background-color: #3C73F1;
  height: 90rpx;
  line-height: 90rpx;
  border-radius:45rpx;
  font-size: 30rpx;
  text-align: center;
  border: 1rpx solid #fff;
  box-shadow: 2rpx 5rpx 10rpx #C4C6C3;
  position: absolute;
  bottom: 100rpx;
  left: 30rpx;
  width: 690rpx;
}