/**app.wxss**/
page, view, text, navigator, image, button, scroll-view {
  box-sizing: border-box;
} 
.container {
  height: 100%;
} 
.flex{
  display: flex;
  align-items: center;
}
.flex_between{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.null {
  /* position: absolute;
  left: 38%;
  bottom: 0; */
  text-align: center;
  font-size: 24rpx;
  color: #ccc;
  padding-top: 20rpx;
}
view {
  word-break:break-all;
}
.line2 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}